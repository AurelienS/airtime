package chart

import "github.com/AurelienS/cigare/web/viewmodel"

templ CountCumul(data viewmodel.CountData) {
	<div id="chart_count">
		<div class="flex justify-between">
			<h2 class="text-2xl">Nombre de vol</h2>
			@CumulativeOrDistinct("cumul","statistics/count", "#chart_count")
		</div>
		@Card() {
			@LineCount(data, "chart_count_canvas")
		}
	</div>
}
