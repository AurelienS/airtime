package chart

import "github.com/AurelienS/cigare/web/viewmodel"

templ TimeDistinct(data []viewmodel.TimeDataset) {
	<div id="chart_time">
		<div class="flex justify-between">
			<h2 class="text-2xl">Temps de vol</h2>
			@CumulativeOrDistinct("distinct","statistics/time", "#chart_time")
		</div>
		@Card() {
			@BarTime(data, "chart_time_canvas")
		}
	</div>
}
